<?xml version="1.0" encoding="UTF-8"?>
<objecttypes>
 <objecttype name="Trigger" color="#00ff00">
  <property name="collidable" type="bool" default="true"/>
  <property name="objectsinitiallayer" type="string"/>
  <property name="sensor" type="bool" default="true"/>
  <property name="triggertype" type="string"/>
 </objecttype>
 <objecttype name="Camera" color="#0000ff">
  <property name="collidable" type="bool" default="true"/>
  <property name="pathfinder" type="string" default="linear1way"/>
  <property name="pathmode" type="string" default="relative"/>
  <property name="script" type="string" default="ShmupCam"/>
