  <property name="speed" type="float" default="240"/>
  <property name="tileid" type="string" default="arrow"/>
  <property name="tileset" type="string" default="humanshots"/>
 </objecttype>
 <objecttype name="Archer" color="#ff0000">
  <property name="convertible" type="bool" default="false"/>
  <property name="firearc" type="float" default="120"/>
  <property name="firebullet" type="string" default="BulletArrow"/>
  <property name="firetime" type="float" default="1"/>
  <property name="health" type="int" default="8"/>
  <property name="kolaunch" type="bool" default="false"/>
  <property name="pathfinder" type="string" default="linear2way"/>
  <property name="pathid" type="int" default="0"/>
